FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../constant/simParams"

application     chtMultiRegionSimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         $nIter;

deltaT          1;

writeControl    timeStep;

writeInterval   $nIter;

purgeWrite      0;

writeFormat     binary;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable true;


functions
{
    #include "probesDict"
limitT
{
    type            limitFields;
    libs            (fieldFunctionObjects);

    fields          (T);
    limit           both;
    min             299;
    max             5000;
    log             false;
    writeControl    none;
}
}


