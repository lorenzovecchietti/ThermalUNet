FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/pcb";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    empty
    {
        type            empty;
    }
    "pcb_to_component_.*"
    {
        type            zeroGradient;
        value           uniform 0;
    }
    pcb_to_fluid
    {
        type            zeroGradient;
        value           uniform 0;
    }
}


