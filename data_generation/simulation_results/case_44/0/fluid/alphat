FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0.5;

boundaryField
{
    empty
    {
        type            empty;
    }
    walls
    {
        type            compressible::alphatWallFunction;;
        value           uniform 0;
    }
    inlet
    {
        type            calculated;
        value           $internalField;
    }
    outlet
    {
        type            calculated;
        value           $internalField;
    }
    "fluid_to_component_.*"
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    fluid_to_pcb
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
}


// ************************************************************************* //
