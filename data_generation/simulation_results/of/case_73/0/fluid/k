FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.5;

boundaryField
{
    empty
    {
        type            empty;
    }
    walls
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.00375;
        value           uniform 0.00375;
    }
    "fluid_to_component_.*"
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    fluid_to_pcb
    {
        type            kqRWallFunction;
        value           $internalField;
    }
}


// ************************************************************************* //
