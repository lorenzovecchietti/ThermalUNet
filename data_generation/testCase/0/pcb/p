
FoamFile
{
    version 2.0;
    format ascii;
    class volScalarField;
    location "0/fluid";
    object p;
}
dimensions      [1 -1 -2 0 0 0 0];
internalField   uniform 101325;
boundaryField
{
    inlet
    {
        type fixedFluxPressure;
        value $internalField;
    }
    outlet
    {
        type fixedValue;
        value $internalField;
    }
    walls
    {
        type zeroGradient;
        value $internalField;
    }
    empty
    {
        type empty;
    }
    "pcb_to_component.*"
    {
        type zeroGradient;
    }
    pcb_to_fluid
    {
        type zeroGradient;
    }
    "fluid_to_heatsink.*"
    {
        type            mappedFixedInternalValue;
        value $internalField;
    }
}
